{
    "general": {
        "outfile": "outfile.{}.csv",
        "gens": 1,
        "burnin": 0,
        "reps": 1,
        "debug": 0,
        "output per": 0
    },

    "population": {
        "N": 100,
        "loci": 32,
        "sex ratio": 0.5,

        "init": {
            "model": "unique"
        },

        "mating": {
            "model": "pure hermaphrodite",
            "a": 0.4,
            "tau": 0.5
        },

        "r": 0.5,
        "mutation": {
            "model": "infinite alleles",
            "theta": [{"value": 0.5, "times": 16}, {"value": 1.5, "times": 16}]
        }
    },

    "post process": {
        "command": ["phase", "rmes"],
        "sample at": "last",
        "clean": true,
        "sample size": 70,
        "reps": 10
    }

}
