#!/usr/bin/env python2

# standard imports
import argparse

# project-specific imports
from selfingsim import analyze
from selfingsim import convert
from selfingsim import sample

def main():
    p = argparse.ArgumentParser()
    sp = p.add_subparsers()
    analyze.setup_command_line(sp)
    convert.setup_command_line(sp)
    sample.setup_command_line(sp)
    a = p.parse_args()
    a.func(a)

if __name__ == '__main__':
    main()
